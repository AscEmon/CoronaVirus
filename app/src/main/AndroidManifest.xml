<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coronavirus">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/coro"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coro"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Emergency_contact"></activity>
        <activity android:name=".Awareness_activity" />
        <activity android:name=".world_activity" />
        <activity android:name=".Bangladesh_activity" />
        <activity android:name=".Special" />
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".SplashScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>